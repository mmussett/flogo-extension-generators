{
	"title": "${{values.triggerName}} Trigger",
	"name": "${{values.triggerName}}",
	"author": "Created by Flogo Extension Generator",
	"version": "1.2.2",
	"type": "flogo:trigger",
	"display": {
		"description": "${{values.triggerName}} trigger",
		"category": "${{values.category}}",
		"visible": true,
		"smallIcon": "icons/trigger-icon@2x.png",
		"largeIcon": "icons/trigger-icon@3x.png",
		"tags": ["${{values.category}}"]
	},
	"ref": "github.com/${{values.owner}}/flogo-enterprise-hub/extensions/${{values.extensionName}}/trigger/${{values.triggerName}}",
	"handler": {
		"settings": [
{%- if(values.hasSettingsString === 'yes') %}
			{
                "name": "aSettingString",
                "type": "string",
                "display": {
                    "name": "String",
				    "description": "This description appears on tooltip hover.",
					"required": true,
                    "type": "string",
                    "appPropertySupport": true
                }
            }
{%- endif %}			
{%- if(values.hasSettingsString === 'yes' and (values.hasSettingsBoolean === 'yes' or values.hasSettingsDropdown === 'yes' or values.hasSettingsDateTime === 'yes' or values.hasSettingsInteger === 'yes' or values.hasSettingsNumber === 'yes')) %}
			,
{%- endif %}
{%- if(values.hasSettingsBoolean === 'yes') %}
			{
				"name": "aSettingBoolean",
				"type": "boolean",
				"display": {
					"name": "Boolean",
					"description": "This description appears on tooltip hover."
				},			
				"value": false,
				"required": true
			}
{%- endif %}
{%- if(values.hasSettingsBoolean === 'yes' and (values.hasSettingsDropdown === 'yes' or values.hasSettingsDateTime === 'yes' or values.hasSettingsInteger === 'yes' or values.hasSettingsNumber === 'yes')) %}       
        	,
{%- endif %}        
{%- if(values.hasSettingsDropdown === 'yes') %}
			{
                "name": "aSettingStringDropdown",
                "type": "string",
                "required": true,
                "allowed": [
                    "Option 1",
                    "Option 2",
					"Option 3"
                ],
				"value": "Option 3",
                "display": {
                    "name": "Dropdown Options",
                    "description": "This description appears on tooltip hover.",
                    "type": "dropdown",
                    "selection": "single"
                }
            }  
{%- endif %}
{%- if(values.hasSettingsDropdown === 'yes' and (values.hasSettingsDateTime === 'yes' or values.hasSettingsInteger === 'yes' or values.hasSettingsNumber === 'yes')) %}       
			,
{%- endif %} 
{%- if(values.hasSettingsDateTime === 'yes') %}       
			{
				"name": "aSettingDateTime",
				"type": "string",
				"display": {
					"name": "Date Time",
					"type": "dateTime",
					"description": "This description appears on tooltip hover.",
					"appPropertySupport": true
				},
        		"wizard": {
          			"step": "Configurations"
        		}
			} 
{%- endif %}
{%- if(values.hasSettingsDateTime === 'yes' and (values.hasSettingsInteger === 'yes' or values.hasSettingsNumber === 'yes')) %}       
			,
{%- endif %} 
{%- if(values.hasSettingsInteger === 'yes') %}       
			{
				"name": "aSettingInteger",
				"type": "integer",
				"required": true,
				"value": 1,
				"display": {
					"name": "Integer",
					"description": "This description appears on tooltip hover.",
					"appPropertySupport": true
				}
			}
{%- endif %}
{%- if(values.hasSettingsInteger === 'yes' and (values.hasSettingsNumber === 'yes')) %}       
			,
{%- endif %}
{%- if(values.hasSettingsNumber === 'yes') %}       
			{
				"name": "aSettingNumber",
				"type": "number",
				"required": true,
				"value": 1.0,
				"display": {
					"name": "Numeric",
					"description": "This description appears on tooltip hover.",
					"appPropertySupport": true
				}
			}
{%- endif %}	
		]
	},
 	"outputs": [
{%- if(values.hasOutputString === 'yes') %}
        {
           "name": "anOutputString",
           "type": "string"
        }
{%- endif %} 
{%- if(values.hasOutputString === 'yes' and (values.hasOutputBoolean === 'yes' or values.hasOutputInteger === 'yes'  or values.hasOutputDecimal === 'yes' or values.hasOutputObject === 'yes' or values.hasOutputObjectWithSchema === 'yes')) %}                       
        ,
{%- endif %}
{%- if(values.hasOutputBoolean === 'yes') %}
        {
            "name": "anOutputBoolean",
            "type": "boolean"
        }
{%- endif %} 
{%- if(values.hasOutputBoolean === 'yes' and (values.hasOutputInteger === 'yes'  or values.hasOutputDecimal === 'yes' or values.hasOutputObject === 'yes' or values.hasOutputObjectWithSchema === 'yes')) %}               
        ,
{%- endif %}
{%- if(values.hasOutputInteger === 'yes') %}
        {
            "name": "anOutputInteger",
            "type": "integer"
        }
{%- endif %} 
{%- if(values.hasOutputInteger === 'yes'  and (values.hasOutputDecimal === 'yes' or values.hasOutputObject === 'yes' or values.hasOutputObjectWithSchema === 'yes')) %}             
        ,
{%- endif %}
{%- if(values.hasOutputDecimal === 'yes') %}
        {
            "name": "anOutputDecimal",
            "type": "number"
        }
{%- endif %} 
{%- if(values.hasOutputDecimal === 'yes' and (values.hasOutputObject === 'yes' or values.hasOutputObjectWithSchema === 'yes')) %}          
        ,
{%- endif %}
{%- if(values.hasOutputObject === 'yes') %}
        {
           "name": "anOutputObject",
           "type": "object"
        }
{%- endif %} 
{%- if(values.hasOutputObject === 'yes' or values.hasOutputObjectWithSchema === 'yes') %}          
        ,
{%- endif %}
{%- if(values.hasOutputObjectWithSchema === 'yes') %}
        {
            "name": "anOutputObjectWithSchema",
            "type": "object",
            "display": {
                "name": "JSON Schema",
                "description": "An example JSON object",
                "type": "texteditor",
                "syntax": "json"
            }
        }
{%- endif %}
  	]
}